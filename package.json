{
	"name": "create-vue3-template",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-only",
		"build-only": "vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"type-check": "vue-tsc --noEmit",
		"changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog.config.js",
		"lint-staged": "lint-staged",
		"lint": "eslint --cache --cache-location node_modules/.cache/.eslintcache --ext .vue,.ts,.tsx,.js,.jsx,.json --quiet src/",
		"lint:fix": "eslint --cache --cache-location node_modules/.cache/.eslintcache --ext .vue,.ts,.tsx,.js,.jsx,.json --quiet src/ --fix",
		"stylelint": "stylelint \"src/**/*.{css,less,vue}\" --cache --cache-location node_modules/.cache/.stylelintcache",
		"stylelint:fix": "stylelint \"src/**/*.{css,less,vue}\" --cache --cache-location node_modules/.cache/.stylelintcache --fix",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"coverage": "vitest run --coverage"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": "请使用pnpm https://pnpm.io/zh/installation",
		"yarn": "请使用pnpm https://pnpm.io/zh/installation",
		"pnpm": ">=8.0.0"
	},
	"dependencies": {
		"@types/lodash-es": "^4.17.6",
		"axios": "^1.2.0",
		"echarts": "^5.5.1",
		"element-plus": "^2.2.26",
		"lodash-es": "^4.17.21",
		"mockjs": "^1.1.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.26",
		"spark-md5": "^3.0.2",
		"vue": "^3.2.45",
		"vue-i18n": "^9.1.10",
		"vue-router": "^4.1.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@eslint/js": "^9.21.0",
		"@iconify-json/ep": "^1.2.2",
		"@types/node": "^18.11.10",
		"@vitejs/plugin-vue": "^5.2.3",
		"@vitejs/plugin-vue-jsx": "^4.1.2",
		"@vitest/coverage-v8": "^3.0.9",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.1.3",
		"autoprefixer": "^10.4.14",
		"compare-func": "^2.0.0",
		"conventional-changelog-cli": "^2.2.2",
		"conventional-changelog-custom-config": "^0.3.1",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-vue": "^10.0.0",
		"globals": "^16.0.0",
		"husky": "^8.0.0",
		"jsdom": "^26.0.0",
		"less": "^4.1.3",
		"lint-staged": "^15.4.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.23",
		"postcss-html": "^1.8.0",
		"postcss-less": "^6.0.0",
		"sharp": "^0.33.5",
		"stylelint": "^16.15.0",
		"stylelint-config-recommended-vue": "^1.6.0",
		"stylelint-less": "^3.0.1",
		"stylelint-order": "^6.0.4",
		"tailwindcss": "^3.3.2",
		"typescript": "~4.9.5",
		"typescript-eslint": "^8.26.0",
		"unplugin-auto-import": "^0.15.3",
		"unplugin-element-plus": "^0.7.2",
		"unplugin-icons": "^22.1.0",
		"unplugin-vue-components": "^0.24.1",
		"vite": "^5.4.14",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-image-optimizer": "^1.1.8",
		"vite-plugin-svg-icons": "^2.0.1",
		"vitest": "^3.0.9",
		"vue-tsc": "^1.0.9"
	},
	"lint-staged": {
		"*.{vue,ts,tsx,js,jsx,json}": [
			"eslint --cache --quiet"
		],
		"*.{css,less,vue}": [
			"stylelint --cache --quiet"
		]
	}
}
